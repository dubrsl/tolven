<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
	<enterprise-beans>
		<session>
			<ejb-name>AdminAppQueueBean</ejb-name>
			<ejb-class>org.tolven.queue.bean.AdminAppQueueBean</ejb-class>
			<session-type>Stateless</session-type>
			<message-destination-ref>
				<message-destination-ref-name>queue/adminApp</message-destination-ref-name>
				<message-destination-type>javax.jms.Queue</message-destination-type>
				<message-destination-usage>Produces</message-destination-usage>
				<message-destination-link>AdminAppQueue</message-destination-link>
			</message-destination-ref>
		</session>
		<session>
			<ejb-name>GeneratorQueueBean</ejb-name>
			<ejb-class>org.tolven.queue.bean.GeneratorQueueBean</ejb-class>
			<session-type>Stateless</session-type>
			<message-destination-ref>
				<message-destination-ref-name>queue/generator</message-destination-ref-name>
				<message-destination-type>javax.jms.Queue</message-destination-type>
				<message-destination-usage>Produces</message-destination-usage>
				<message-destination-link>GeneratorQueue</message-destination-link>
			</message-destination-ref>
		</session>
		<session>
			<ejb-name>InvitationQueueBean</ejb-name>
			<ejb-class>org.tolven.queue.bean.InvitationQueueBean</ejb-class>
			<session-type>Stateless</session-type>
			<message-destination-ref>
				<message-destination-ref-name>queue/invitation</message-destination-ref-name>
				<message-destination-type>javax.jms.Queue</message-destination-type>
				<message-destination-usage>Produces</message-destination-usage>
				<message-destination-link>InvitationQueue</message-destination-link>
			</message-destination-ref>
		</session>
		<session>
			<ejb-name>RuleQueueBean</ejb-name>
			<ejb-class>org.tolven.queue.bean.RuleQueueBean</ejb-class>
			<session-type>Stateless</session-type>
			<message-destination-ref>
				<message-destination-ref-name>queue/rule</message-destination-ref-name>
				<message-destination-type>javax.jms.Queue</message-destination-type>
				<message-destination-usage>Produces</message-destination-usage>
				<message-destination-link>RuleQueue</message-destination-link>
			</message-destination-ref>
		</session>
		<message-driven>
			<ejb-name>AdminAppMDB</ejb-name>
			<ejb-class>org.tolven.app.bean.AdminAppMDB</ejb-class>
			<message-destination-type>javax.jms.Queue</message-destination-type>
			<message-destination-link>AdminAppQueue</message-destination-link>
		</message-driven>
		<message-driven>
			<ejb-name>Evaluator</ejb-name>
			<ejb-class>org.tolven.doc.bean.Evaluator</ejb-class>
			<message-destination-type>javax.jms.Queue</message-destination-type>
			<message-destination-link>RuleQueue</message-destination-link>
		</message-driven>
		<message-driven>
			<ejb-name>GenDriver</ejb-name>
			<ejb-class>org.tolven.gen.bean.GenDriver</ejb-class>
			<transaction-type>Container</transaction-type>
			<message-destination-type>javax.jms.Queue</message-destination-type>
			<message-destination-link>GeneratorQueue</message-destination-link>
		</message-driven>
		<message-driven>
			<ejb-name>InvitationSender</ejb-name>
			<ejb-class>org.tolven.core.bean.InvitationSender</ejb-class>
			<message-destination-type>javax.jms.Queue</message-destination-type>
			<message-destination-link>InvitationQueue</message-destination-link>
		</message-driven>
	</enterprise-beans>
	<assembly-descriptor>
		<message-destination>
			<message-destination-name>RuleQueue</message-destination-name>
		</message-destination>
		<message-destination>
			<message-destination-name>AdminAppQueue</message-destination-name>
		</message-destination>
		<message-destination>
			<message-destination-name>GeneratorQueue</message-destination-name>
		</message-destination>
		<message-destination>
			<message-destination-name>InvitationQueue</message-destination-name>
		</message-destination>
	</assembly-descriptor>
</ejb-jar>