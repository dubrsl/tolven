<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.tolven.component.tomcatserver" version="0.0.1" vendor="Tolven" xmlns="urn:tolven-org:tpf:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:tpf:1.0 http://tolven.org/xsd/tpf.xsd">
	<extension-point id="tomcatserverxmlAssembly" extension-multiplicity="one">
		<parameter-def id="connectorDefinition" default-value="connector" multiplicity="none-or-one" />
		<parameter-def id="webserverCredentialDirectory" default-value="#{pluginProperty['webserver.credentialDir']}" multiplicity="none-or-one" />
		<parameter-def id="webserverDeploymentDirectory" default-value="#{pluginProperty['webserver.deploymentDir']}" multiplicity="none-or-one" />
	</extension-point>
	<extension-point id="connector">
		<parameter-def id="port" multiplicity="none-or-one" />
		<parameter-def id="address" multiplicity="none-or-one" />
		<parameter-def id="maxThreads" multiplicity="none-or-one" />
		<parameter-def id="maxHttpHeaderSize" multiplicity="none-or-one" />
		<parameter-def id="emptySessionPath" multiplicity="none-or-one" />
		<parameter-def id="protocol" multiplicity="none-or-one" />
		<parameter-def id="enableLookups" multiplicity="none-or-one" />
		<parameter-def id="redirectPort" multiplicity="none-or-one" />
		<parameter-def id="acceptCount" multiplicity="none-or-one" />
		<parameter-def id="connectionTimeout" multiplicity="none-or-one" />
		<parameter-def id="disableUploadTimeout" multiplicity="none-or-one" />
		<parameter-def id="SSLEnabled" multiplicity="none-or-one" />
		<parameter-def id="scheme" multiplicity="none-or-one" />
		<parameter-def id="secure" multiplicity="none-or-one" />
		<parameter-def id="clientAuth" multiplicity="none-or-one" />
		<parameter-def id="sslProtocol" multiplicity="none-or-one" />
		<parameter-def id="keystoreFile" multiplicity="none-or-one" />
		<parameter-def id="keystoreType" multiplicity="none-or-one" />
		<parameter-def id="keystorePass" multiplicity="none-or-one" />
	</extension-point>
</plugin>