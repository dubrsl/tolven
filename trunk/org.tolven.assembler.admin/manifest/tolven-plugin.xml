<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.tolven.assembler.admin" version="2.0.15" class="org.tolven.assembler.admin.AdminAssembler" vendor="Tolven" xmlns="urn:tolven-org:tpf:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:tpf:1.0 http://tolven.org/xsd/tpf.xsd">
	<requires>
		<import plugin-id="org.tolven.library.bouncy-castle" />
	</requires>
	<runtime>
		<library id="tpfclasses" path="tpf/classes/" type="code" />
	</runtime>
	<extension-point id="runtimeLib" extension-multiplicity="none">
		<parameter-def id="serverconfig" default-value="tolvenServerConfig.jar" multiplicity="none-or-one" />
	</extension-point>
	<extension-point id="serverConfig" parent-plugin-id="org.tolven.assembler.admin" parent-point-id="runtimeLib" extension-multiplicity="none">
		<parameter-def id="serverconfig" multiplicity="none-or-one" />
	</extension-point>
	<extension-point id="bcprov" parent-plugin-id="org.tolven.library.bouncy-castle" parent-point-id="runtimeLib" extension-multiplicity="none">
		<parameter-def id="bcprov" multiplicity="none-or-one" />
	</extension-point>
	<extension-point id="authRestful" extension-multiplicity="one">
		<parameter-def id="authRestful.url" />
	</extension-point>
	<extension-point id="appRestful" extension-multiplicity="one">
		<parameter-def id="appRestful.url" />
	</extension-point>
	<extension-point id="devLib" extension-multiplicity="none">
		<parameter-def id="jar" default-value="devLib.jar" />
	</extension-point>
</plugin>