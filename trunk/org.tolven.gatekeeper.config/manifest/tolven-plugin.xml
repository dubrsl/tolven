	<requires>
		<import plugin-id="org.tolven.gatekeeper" />
	</requires>
	
	<!-- LOCAL Resources -->
	<extension-point id="shiro-ini" extension-multiplicity="none">
		<parameter-def id="jar" default-value="shiro-ini.jar" multiplicity="none-or-one" />
	</extension-point>
	<!-- LOCAL Resources End -->
	
	<!-- LIB Extensions -->
	<extension id="shiro-ini" plugin-id="org.tolven.gatekeeper" point-id="libProduct-adaptor">
