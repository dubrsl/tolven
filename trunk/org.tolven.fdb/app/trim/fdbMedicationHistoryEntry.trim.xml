<?xml version="1.0" encoding="UTF-8"?>
<trim xmlns="urn:tolven-org:trim:4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:tolven-org:trim:4.0 http://www.tolven.org/xsd/trim4">
	<name>obs/evn/fdbMedicationHistoryEntry</name>
	<description>Medication History Entry Template(FDB)</description>
	<author>Tolven</author>
	<act moodCode="EVN" classCode="SBADM">
		<bind application="echr" phase="create">
			<placeholder bindAction="merge">
				<path>echr:patient:medication</path>
			</placeholder>
		</bind>
		<bind application="ephr" phase="create">
			<placeholder bindAction="merge">
				<path>ephr:patient:medication</path>
			</placeholder>
		</bind>
		<effectiveTime>
			<IVL_TS>
				<low>
					<label>Start Date</label>
					<TS />
				</low>
				<high>
					<label>End Date</label>
					<TS />
				</high>
			</IVL_TS>
		</effectiveTime>
		<procedure>
			<substanceAdministration>
				<routeCode>
					<valueSet>route</valueSet>					
				</routeCode>
			</substanceAdministration>
		</procedure>
		
		<participation name="consumableProduct" typeCode="CON">
			<role classCode="THER">
				<player classCode="MMAT" determinerCode="INSTANCE">
					<code>
						<CE/>
					</code>
				</player>
			</role>
		</participation>
		
		<relationship name="frequency" typeCode="PERT" direction="OUT">
			<act classCode="ACT" moodCode="EVN">
				<observation>
					<value>
						<label>frequency</label>
						<valueSet>frequency</valueSet>
					</value>
				</observation>
			</act>
		</relationship>
		<relationship name="instructions" typeCode="PERT" direction="OUT">
			<act classCode="ACT" moodCode="EVN">
				<title>
					<ST>Administration Instructions</ST>
				</title>
				<text>
					<originalText></originalText>
				</text>
			</act>
		</relationship>
		<relationship name="comments" typeCode="PERT" direction="OUT">
			<act classCode="ACT" moodCode="EVN">
				<title>
					<ST>Comments</ST>
				</title>
				<text>
					<originalText></originalText>
				</text>
			</act>
		</relationship>
	</act>

</trim>
