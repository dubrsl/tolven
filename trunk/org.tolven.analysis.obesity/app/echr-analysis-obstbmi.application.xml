<application xmlns="urn:tolven-org:menuStructure:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:menuStructure:1.0 http://tolven.org/xsd/menuStructure.xsd">
	<property accountType="echr" name="org.tolven.cohort.obst.includeCodes"></property>
	<property accountType="echr" name="org.tolven.cohort.obst.excludeCodes"></property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.target">90</property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.intervalValue">1</property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.intervalUnit">Month</property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.chartRange">12</property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.chartDataTitle">% Patients</property>
	<property accountType="echr" name="org.tolven.cohort.obst.bmisnapshot.chartTargetTitle">Target</property>
	<extends path="echr:cohort">
		<placeholder name="bmisnapshot" title="BMI Recording Snapshot" page="/five/snapshot.xhtml" heading="/five/snapshotLink.xhtml">
			<field name="name" internal="string01" />
			<field name="effectiveTime" internal="date01">
				<from>#{act.effectiveTime.TS.date}</from>
				<from>#{act.effectiveTime.IVLTS.low.date}</from>
			</field>
			<field name="cohort" internal="parent01">
				<from>#{cohort}</from>
			</field>
			<field name="targetPercent" internal="_extended" type="real" />
			<field name="allCount" internal="_extended" type="real" />
			<field name="normCount" internal="_extended" type="real" />
			<field name="abnormCount" internal="_extended" type="real" />
			<list name="all" title="All Results" page="/five/list.xhtml" visible="true" drilldown="echr:cohort:bmisnapshot:bmisnapshotresult">
				<column name="Name" width="20.0">
					<from>#{bmisnapshotresult.patient.lastName}, #{bmisnapshotresult.patient.firstName} #{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="Last" visible="false">
					<from>#{bmisnapshotresult.patient.lastName}</from>
				</column>
				<column name="First" visible="false">
					<from>#{bmisnapshotresult.patient.firstName}</from>
				</column>
				<column name="Middle" visible="false">
					<from>#{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="DOB" format="d MMM yyyy" width="7.0">
					<from>#{bmisnapshotresult.patient.dob}</from>
				</column>
				<column name="Age" width="4.5">
					<from>#{age(bmisnapshotresult.patient.dob, accountUser)}</from>
				</column>
				<column name="Sex" width="6.0">
					<from>#{bmisnapshotresult.patient.sex}</from>
					<from>#{bmisnapshotresult.patient.gender}</from>
				</column>
				<column name="BMIRecord" width="12.0" align="right" visible="true">
					<from>#{bmisnapshotresult.result.pqValue01}</from>
				</column>
			</list>
			<list name="bmi" title="BMI Recorded" page="/five/list.xhtml" visible="true" drilldown="echr:cohort:bmisnapshot:bmisnapshotresult">
				<column name="Name" width="20.0">
					<from>#{bmisnapshotresult.patient.lastName}, #{bmisnapshotresult.patient.firstName} #{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="Last" visible="false">
					<from>#{bmisnapshotresult.patient.lastName}</from>
				</column>
				<column name="First" visible="false">
					<from>#{bmisnapshotresult.patient.firstName}</from>
				</column>
				<column name="Middle" visible="false">
					<from>#{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="DOB" format="d MMM yyyy" width="7.0">
					<from>#{bmisnapshotresult.patient.dob}</from>
				</column>
				<column name="Age" width="4.5">
					<from>#{age(bmisnapshotresult.patient.dob, accountUser)}</from>
				</column>
				<column name="Sex" width="6.0">
					<from>#{bmisnapshotresult.patient.sex}</from>
					<from>#{bmisnapshotresult.patient.gender}</from>
				</column>
				<column name="Last BMI" width="6.0" align="right" visible="true">
					<from>#{bmisnapshotresult.result.pqValue01}</from>
				</column>
			</list>
			<list name="nobmi" title="BMI Not Recorded" page="/five/list.xhtml" visible="true" drilldown="echr:cohort:bmisnapshot:bmisnapshotresult">
				<column name="Name" width="20.0">
					<from>#{bmisnapshotresult.patient.lastName}, #{bmisnapshotresult.patient.firstName} #{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="Last" visible="false">
					<from>#{bmisnapshotresult.patient.lastName}</from>
				</column>
				<column name="First" visible="false">
					<from>#{bmisnapshotresult.patient.firstName}</from>
				</column>
				<column name="Middle" visible="false">
					<from>#{bmisnapshotresult.patient.middleName}</from>
				</column>
				<column name="DOB" format="d MMM yyyy" width="7.0">
					<from>#{bmisnapshotresult.patient.dob}</from>
				</column>
				<column name="Age" width="4.5">
					<from>#{age(bmisnapshotresult.patient.dob, accountUser)}</from>
				</column>
				<column name="Sex" width="6.0">
					<from>#{bmisnapshotresult.patient.sex}</from>
					<from>#{bmisnapshotresult.patient.gender}</from>
				</column>
				<column name="Last BMI" width="6.0" align="right" visible="true">
					<from>#{bmisnapshotresult.result.pqValue01}</from>
				</column>
			</list>
		</placeholder>
	</extends>
	<extends path="echr:cohort:bmisnapshot">
		<placeholder name="bmisnapshotresult" title="BMI Recording Snapshot Result">
			<field name="effectiveTime" internal="date01">
				<from>#{act.effectiveTime.TS.date}</from>
				<from>#{act.effectiveTime.IVLTS.low.date}</from>
			</field>
			<field name="bmisnapshot" internal="parent01">
				<from>#{bmisnapshot}</from>
			</field>
			<field name="patient" internal="parent02">
				<from>#{patient}</from>
			</field>
			<field name="result" internal="parent03">
				<from>#{result}</from>
			</field>
		</placeholder>
	</extends>
	<extends path="echr:cohort:snapshots">
		<list name="bmisnapshot" title="BMI Recording" sequence="500" initialSort="Date" page="/five/list.xhtml" visible="true" drilldown="echr:cohort:bmisnapshot">
			<column name="Name" internal="string01" reference="true" width="25.0">
				<from>#{bmisnapshot.cohort.name}</from>
			</column>
			<column name="Date" internal="date01" format="MMM-dd-yyyy hh:mm:ss" width="15.0">
				<from>#{bmisnapshot.effectiveTime}</from>
			</column>
		</list>
	</extends>
	<extends path="echr:analysis:portal">
		<portlet name="obstbmisnapshotsum" portalColumn="2" sequence="2" title="BMI Recording" page="/five/analysis/obstbmiAnalysis.xhtml" visible="true" drilldown="echr:analysis:obstbmisnapshot" />
	</extends>
