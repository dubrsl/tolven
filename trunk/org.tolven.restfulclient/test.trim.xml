                    <ST>Clothing</ST>
                </title>
                <observation>
                    <value>
                        <label>Clothing</label>
                        <valueSet>clothing</valueSet>
                    </value>
                </observation>
            </act>
        </relationship>
        <relationship direction="OUT" name="device" typeCode="PERT">
            <act classCode="OBS" moodCode="EVN">
                <title>
                    <ST>Device</ST>
                </title>
                <observation>
                    <value>
                        <label>Device</label>
                        <valueSet>device</valueSet>
                    </value>
                </observation>
            </act>
        </relationship>
		<compute>
			<type>org.tolven.process.DataTypeSelect</type>
